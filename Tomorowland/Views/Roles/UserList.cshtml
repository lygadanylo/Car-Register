@model IEnumerable<Tomorowland.Models.Login.User>

<h2>Список користувачів</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступу</a>
            </td>
        </tr>
    }
</table>
<a class="btn btn-sm btn-primary" asp-action="Create">Додати ролі</a>